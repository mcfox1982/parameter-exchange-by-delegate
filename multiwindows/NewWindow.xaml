<Window x:Class="multiwindows.NewWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:multiwindows"
        x:FieldModifier="public"
        mc:Ignorable="d"
        Title="NewWindow" Height="450" Width="800" Name="NewWind"
        >
    <Grid>
        <StackPanel>
            <TextBox Margin="3"  x:Name="TextBoxNew" FontSize="30" Width="Auto" Height="50" />
            <Button Name="ButtonNew" FontSize="30 " Content="Back to main window" Click="ButtonNew_OnClick"/>
        </StackPanel>
       
    </Grid>
</Window>
